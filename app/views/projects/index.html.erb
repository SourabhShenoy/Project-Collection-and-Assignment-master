<% provide(:title, @title.split('_').collect(&:capitalize).join(" ")) %>
<h1><%= @title.split('_').collect(&:capitalize).join(" ") %></h1>

<div class="row">
<div class="col-md-6 col-md-offset-3">
 
<%= will_paginate %>

<table class="projects">
<tr>
<th><%=link_to "Semester",send(@path,:sort => 'year')%></th>
<th><%=link_to "Title",send(@path,:sort => 'title')%></th>
<th>Legacy</th>
<th>On Campus</th>
<% if current_user.admin? %>
<th colspan="3">Admin Panel</th>

<th>Last Modified</th>
<% end %>
</tr>

  <%= render @projects %>
</table>

<%= will_paginate %>

</div>
</div>